<div class="container">
  <div class="col-xs-6">
    <h2>Edit your profile</h2>

    <%= form_for @user,  html: { multipart: true }  do |form| %>
      <%= render partial: '/users/form', object: form %>

      <div class="submit-field">
        <%= form.submit "Save", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>